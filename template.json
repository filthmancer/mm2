							}
						]
					}
				],
				"link": "fire up the lighter?",
				"sectionID": "3"
			}
		},
		{
			"type": "clearline"
		},
		{
			"type": "text",
			"value": "Otherwise,"
		},
		{
			"type": "turnto",
			"value": {
				"conditions": [
					{
						"required": [
							{
								"gameID": "FlickeringLight",
